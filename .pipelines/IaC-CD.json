"","Inline":"$tenantid = \"$(Tenant-ID)\"\n$clientid = \"$(ServicePrincipal-ApplicationID)\"\n$password = \"$(ServicePrincipal-Secret)\"\n$resourceGroupName = \"$(resource-Group)\"\n$databricks  = \"$(RGDO_databricksWorkspaceName)\"\n$url = \"https://$(location).azuredatabricks.net\"\n$keyVaultUrl = \"https://$(RGDO_keyVaultName).vault.azure.net\"\n\n$userPassword = ConvertTo-SecureString -String $password -AsPlainText -Force\n$userCredential = New-Object -TypeName System.Management.Automation.PSCredential -ArgumentList $clientid, $userPassword\nConnect-AzAccount -Credential $userCredential -ServicePrincipal –TenantId $tenantid\n\n$databricksResourceId = (Get-AzResourceGroup -Name \"$(resource-Group)\").ResourceId + \"/providers/Microsoft.Databricks/workspaces/$databricks\"\n\n#GET DATABRICKS TOKEN\n$databricksToken= (Get-AzAccessToken -Resource \"2ff814a6-3304-4ab8-85cb-cd0e6f879c1d\").Token\n\n#GET MANAGEMENT TOKEN\n$managementToken = (Get-AzAccessToken -Resource \"https://management.core.windows.net/\").Token\n\n#GET DATABRICKS KEYVAULT\n$keyVaultToken= (Get-AzAccessToken -Resource \"https://vault.azure.net\").Token\n\n#GET WORKSPACE\n$workspaceUrl=\"https://\"+(Get-AzResource -Name $databricks -ResourceGroupName $resourceGroupName -ExpandProperties).Properties.workspaceUrl\n\n#CREATE PAT\n$headers = @{ \"Authorization\" = \"Bearer $databricksToken\";\n\"X-Databricks-Azure-SP-Management-Token\" = \"$managementToken\";\n\"X-Databricks-Azure-Workspace-Resource-Id\" = \"$databricksResourceId \"\n }\n$body = @{ comment = \"DevOps Created\" } | convertTo-Json\n$databricksPat = (Invoke-RestMethod -Method Post -ContentType \"application/json\" -Uri \"$url/api/2.0/token/create\" -Headers $headers -Body $body).token_value\n\n#STORE DATABRICKS-PAT KEY VAULT\n$headers = @{ \"Authorization\" = \"Bearer $keyVaultToken\" }\n$body = @{ value= \"$databricksPat\" } | convertTo-Json\nInvoke-RestMethod -Method PUT -ContentType \"application/json\" -Uri \"$keyVaultUrl/secrets/Databricks-PAT?api-version=7.2\" -Headers $headers -Body $body\n\n#CREATE CLUSTER\n$headers = @{ \"Authorization\" = \"Bearer $databricksToken\";\n\"X-Databricks-Azure-SP-Management-Token\" = \"$managementToken\";\n\"X-Databricks-Azure-Workspace-Resource-Id\" = \"$databricksResourceId \"\n }\n$body = @{ cluster_name =  \"default-cluster\";\n  spark_version = \"10.1.x-scala2.12\";\n  node_type_id = \"Standard_F4\";\n  autotermination_minutes = 20;\n  num_workers = 1\n} | convertTo-Json\n$clusterId = (Invoke-RestMethod -Method Post -ContentType \"application/json\" -Uri \"$url/api/2.0/clusters/create\" -Headers $headers -Body $body).cluster_id\n\n#STORE DATABRICKS-CLUSTERID KEY VAULT\n$headers = @{ \"Authorization\" = \"Bearer $keyVaultToken\" }\n$body = @{ value= \"$clusterId\" } | convertTo-Json\nInvoke-RestMethod -Method PUT -ContentType \"application/json\" -Uri \"$keyVaultUrl/secrets/Databricks-ClusterId?api-version=7.2\" -Headers $headers -Body $body\n","ScriptArguments":"","errorActionPreference":"stop","FailOnStandardError":"false","TargetAzurePs":"LatestVersion","CustomTargetAzurePs":"","pwsh":"false","workingDirectory":""}},{"environment":{},"taskId":"72a1931b-effb-4d2e-8fd8-f8472a07cb62","version":"5.*","name":"Synapse Setup","refName":"","enabled":true,"alwaysRun":false,"continueOnError":false,"timeoutInMinutes":0,"retryCountOnTaskFailure":0,"definitionType":"task","overrideInputs":{},"condition":"ne(variables['RGDO_synapseWorkspaceName'], '')","inputs":{"ConnectedServiceNameARM":"21925b97-d3b2-4c4a-a369-8baa55422b49","ScriptType":"InlineScript","ScriptPath":"","Inline":"Install-Module -Name Az.Synapse -Scope CurrentUser -Force \n\n$tenantid = \"$(Tenant-ID)\"\n$clientid = \"$(ServicePrincipal-ApplicationID)\"\n$Password = \"$(ServicePrincipal-Secret)\"\n$workspaceName = \"$(RGDO_synapseWorkspaceName)\"\n\n$userPassword = ConvertTo-SecureString -String $password -AsPlainText -Force\n$userCredential = New-Object -TypeName System.Management.Automation.PSCredential -ArgumentList $clientid, $userPassword\n\nConnect-AzAccount -Credential $userCredential -ServicePrincipal –TenantId $tenantid\n\n#Bearer token\n$token = (Get-AzAccessToken -Resource \"https://dev.azuresynapse.net\").Token\n$headers = @{ Authorization = \"Bearer $token\" }\n\n#get roles\n$uri=\"https://$workspacename.dev.azuresynapse.net/rbac/roles?api-version=2020-02-01-preview\"\n\n$result = Invoke-RestMethod -Method Get -ContentType \"application/json\" -Uri $uri -Headers $headers | ConvertTo-json\n$Pscustom = ConvertFrom-Json –InputObject $result \n$object = $Pscustom.value | where {$_.name -eq 'Workspace Admin'}\n$id = $object.id\n\n#Assign role\n\n$uri1 = \"https://$workspacename.dev.azuresynapse.net/rbac/roleAssignments?api-version=2020-02-01-preview\"\n$body = @{\nroleId = $id\n\nprincipalId= \"$(AAD-Group-ObjectID)\"\n} | convertTo-Json\n    \n$pos = Invoke-RestMethod -Method Post -ContentType \"application/json\" -Uri $uri1 -Headers $headers -Body $body\n\nSuspend-AzSynapseSqlPool -WorkspaceName \"$(RGDO_synapseWorkspaceName)\" -Name \"EDW_PRES\"\n","ScriptArguments":"","errorActionPreference":"stop","FailOnStandardError":"false","TargetAzurePs":"LatestVersion","CustomTargetAzurePs":"","pwsh":"false","workingDirectory":""}},{"environment":{},"taskId":"ce85a08b-a538-4d2b-8589-1d37a9ab970f","version":"1.*","name":"ControlDB","refName":"","enabled":true,"alwaysRun":false,"continueOnError":false,"timeoutInMinutes":0,"retryCountOnTaskFailure":0,"definitionType":"task","overrideInputs":{},"condition":"succeeded()","inputs":{"ConnectedServiceNameSelector":"ConnectedServiceNameARM","ConnectedServiceName":"","ConnectedServiceNameARM":"21925b97-d3b2-4c4a-a369-8baa55422b49","AuthenticationType":"server","ServerName":"$(RGDO_AzureSqlServerName).database.windows.net","DatabaseName":"ControlDB","SqlUsername":"$(RGDO_dbUsername)","SqlPassword":"$(RGDO_dbPassword)","aadSqlUsername":"","aadSqlPassword":"","ConnectionString":"","TaskNameSelector":"DacpacTask","DeploymentAct